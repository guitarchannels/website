{
	"name": "Sailing-Channels",
	"description": "Discovering YouTube sailing channels by using a network analysis on recursive channel subscriptions",
	"version": "1.14.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/thomasbrueggemann/sailing-channels.git"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=development node server",
		"postinstall": "bower install",
		"build": "npm run build:clean && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
		"build:clean": "rimraf ./docs",
		"precommit": "pretty-quick --staged"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"modules": false
				}
			],
			"react",
			"stage-0"
		],
		"env": {
			"development": {
				"only": [
					"app"
				],
				"plugins": [
					"transform-react-jsx-source",
					"transform-es2015-modules-commonjs"
				]
			},
			"production": {
				"only": [
					"app"
				],
				"plugins": [
					"transform-react-remove-prop-types",
					"transform-react-constant-elements",
					"transform-react-inline-elements"
				]
			},
			"test": {
				"plugins": [
					"transform-es2015-modules-commonjs",
					"dynamic-import-node"
				]
			}
		}
	},
	"dependencies": {
		"ais": "^1.2.1",
		"alt": "^0.17.4",
		"anchorme": "^1.1.2",
		"apicache": "^0.7.2",
		"async": "^1.4.2",
		"babel": "^5.8.23",
		"body-parser": "^1.14.1",
		"bootstrap": "3.3.5",
		"bootstrap-material-design": "^0.5.7",
		"colors": "^1.1.2",
		"compression": "^1.6.0",
		"cookie-parser": "1.4.0",
		"envify": "^3.4.0",
		"express": "^4.13.3",
		"font-awesome": "^4.7.0",
		"history": "^1.12.5",
		"immutable": "^3.7.6",
		"inreach": "^1.1.3",
		"ip": "1.1.5",
		"iso-639-1": "^1.0.0",
		"jquery": "^3.3.1",
		"jsonfile": "^2.2.3",
		"minimist": "^1.2.0",
		"moment": "2.22.2",
		"mongodb": "2.0.48",
		"parse-youtube-user": "0.0.2",
		"prop-types": "^15.6.1",
		"react": "^0.14.7",
		"react-cookie": "^0.4.5",
		"react-dom": "^0.14.8",
		"react-helmet": "^2.3.1",
		"react-infinite": "^0.8.0",
		"react-lazyload": "^2.2.7",
		"react-loadable": "5.3.1",
		"react-router": "^1.0.0-rc3",
		"request": "^2.65.0",
		"servertiming": "^1.1.2",
		"timeago": "^1.6.3",
		"validator": "^5.1.0",
		"youtube-api": "^1.4.0",
		"youtube-custom-links": "^1.0.2"
	},
	"devDependencies": {
		"add-asset-html-webpack-plugin": "2.0.1",
		"babel-cli": "6.24.1",
		"babel-core": "6.24.1",
		"babel-loader": "7.1.0",
		"babel-plugin-dynamic-import-node": "1.0.2",
		"babel-plugin-react-intl": "2.3.1",
		"babel-plugin-react-transform": "2.0.2",
		"babel-plugin-styled-components": "1.1.4",
		"babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
		"babel-plugin-transform-react-constant-elements": "6.23.0",
		"babel-plugin-transform-react-inline-elements": "6.22.0",
		"babel-plugin-transform-react-remove-prop-types": "0.4.5",
		"babel-preset-env": "1.5.1",
		"babel-preset-react": "6.24.1",
		"babel-preset-stage-0": "6.24.1",
		"bower": "^1.6.3",
		"circular-dependency-plugin": "3.0.0",
		"cross-env": "^5.1.6",
		"css-loader": "0.28.4",
		"eventsource-polyfill": "0.9.6",
		"exports-loader": "0.6.4",
		"file-loader": "0.11.1",
		"html-loader": "0.4.5",
		"html-webpack-plugin": "2.29.0",
		"husky": "^0.14.3",
		"image-webpack-loader": "2.0.0",
		"imports-loader": "0.7.1",
		"node-plop": "0.7.0",
		"null-loader": "0.1.1",
		"offline-plugin": "4.8.1",
		"plop": "1.8.0",
		"prettier": "^1.12.1",
		"pretty-quick": "^1.6.0",
		"react-test-renderer": "15.6.1",
		"rimraf": "2.6.1",
		"shelljs": "^0.7.8",
		"style-loader": "0.18.1",
		"url-loader": "0.5.8",
		"webpack": "3.5.5",
		"webpack-dev-middleware": "1.11.0",
		"webpack-hot-middleware": "2.18.0"
	},
	"license": "MIT",
	"prettier": {
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 4,
		"arrowParens": "always"
	}
}
